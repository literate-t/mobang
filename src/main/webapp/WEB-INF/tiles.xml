<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">

<tiles-definitions>
	<definition name="mobang.*" template="/WEB-INF/view/inc/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/view/inc/header.html" />
		<put-attribute name="main" value="/WEB-INF/view/mobang/{1}.html" />
	</definition>
	
	<definition name="mobang.*.*" template="/WEB-INF/view/inc/layout.jsp">
		<put-attribute name="header" value="/WEB-INF/view/inc/header.html" />
		<put-attribute name="filter" value="/WEB-INF/view/mobang/search/filter.html" />
		<put-attribute name="main" value="/WEB-INF/view/mobang/{1}/{2}.html" />
	</definition>
</tiles-definitions>